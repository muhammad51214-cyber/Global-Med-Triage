# Copy this file to .env and fill in real values. Never commit real secrets.

# --- Core application settings ---
JWT_SECRET=change_me_dev_secret                # Secret for JWT authentication (backend)
DATABASE_URL=postgresql://globalmed:changeme@db:5432/triage  # PostgreSQL connection string

# --- Agent & API keys (placeholders) ---
GEMINI_API_KEY=your_gemini_api_key_here        # Translation agent (Gemini)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here  # Voice interface agent (ElevenLabs)
MISTRAL_API_KEY=your_mistral_api_key_here      # Medical triage agent (Mistral)
CROSSMINT_API_KEY=your_crossmint_api_key_here  # Insurance verification agent (Crossmint)
ML_API_URL=http://localhost:8000/mock-ml-api   # Vital signs monitor agent (ML API, local mock)
MEDICAL_OFFICE_TRIAGE_API_URL=http://localhost:8010/collect-history  # Medical office triage voice agent (Coral FastAPI endpoint)

# --- Optional runtime tuning ---
PORT=8000
LOG_LEVEL=info

